<Window x:Class="HB.CusControlMainApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HB.CusControlMainApp"
        xmlns:hbc="clr-namespace:HB.CusControls.Controls;assembly=HB.CusControls"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <hbc:HBTimePiker x:Name="timepicker" Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Button x:Name ="test" Grid.Row="1" Content="ceshi" Click="test_Click"/>
        <TextBlock Grid.Row="2" Text="{Binding TimePickerTime}" />
    </Grid>
</Window>
